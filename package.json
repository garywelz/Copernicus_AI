{
   "name": "copernicus-ai",
   "version": "1.0.0",
   "description": "AI-powered podcast and video generation system for educational content",
   "main": "dist/index.js",
   "types": "dist/index.d.ts",
   "scripts": {
      "build": "tsc",
      "start": "node dist/index.js",
      "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
      "test": "jest",
      "lint": "eslint . --ext .ts",
      "format": "prettier --write \"src/**/*.ts\"",
      "clean": "rimraf dist",
      "prepare": "npm run clean && npm run build"
   },
   "dependencies": {
      "@google-cloud/aiplatform": "^3.9.0",
