      "bcrypt": "^5.1.1",
      "chalk": "^4.1.2",
      "dotenv": "^16.4.1",
      "express": "^4.18.2",
      "fast-xml-parser": "^4.3.3",
      "fluent-ffmpeg": "^2.1.2",
      "get-audio-duration": "^4.0.1",
      "get-video-metadata": "^1.0.5",
      "jsonwebtoken": "^9.0.2",
      "node-fetch": "^3.3.2",
      "openai": "^4.28.0",
      "stripe": "^14.18.0",
      "typescript": "^5.3.3",
      "winston": "^3.11.0"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.2",
